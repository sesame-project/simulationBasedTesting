@namespace(
	uri="ExSceMM",
	prefix="ExSceMM")
package top;

class MRS {
    val Topic[*] topics;
	attr String launchFileLocation;
	val Node[*] nodes;
	val Simulator[1] simulator;
}

class Node {
	attr String name;
	ref Topic[*] subscriber;
	ref Topic[*] publisher;
}

class Topic {
	attr String name;
}

abstract class Simulator {
}

class ROSSimulator extends Simulator {
	attr String hostname;
	attr String port;
}

